{
    "name": "agontuk/schema-builder",
    "description": "Schema designer & migration generator for laravel & lumen",
    "version": "1.4.0",
    "type": "library",
    "keywords": [
        "schema",
        "database",
        "schema builder",
        "schema design",
        "migration generator"
    ],
    "homepage": "https://github.com/Agontuk/schema-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Iftekhar Rifat",
            "email": "rifat662@gmail.com",
            "role": "Developer",
            "homepage": "https://github.com/Agontuk"
        }
    ],
    "require": {
        "ext-zip": "*",
        "illuminate/filesystem": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/support": "^9.0",
        "league/flysystem": "^2.0",
        "league/flysystem-ziparchive": "^2.0"
    },
    "require-dev": {
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Agontuk\\Schema\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Agontuk\\Schema\\SchemaServiceProvider"
            ]
        }
    }
}
